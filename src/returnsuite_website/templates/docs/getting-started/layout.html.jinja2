{% extends "docs/layout.html.jinja2" %}
{% set page_title = "Documentation - " + app_title %}
{% set documentation_section = "getting-started" %}

{% block content %}
  <script>
  function menu() {
    return {
      menu_open: true,
      table_of_contents: [
        {
          "title": "Overview",
          "href": "#",
          "selected": {{ (context_page_name == "get_getting_started_overview")|tojson }}
        },
        {
          "title": "Creating an account",
          "href": "#",
          "selected": {{ (context_page_name == "get_getting_started_signup_walkthrough")|tojson }}
        }, {
          "collapsed": true,
          "title": "Starting with a sample property",
          "items": [
            {
              "title": "Selecting a sample",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_sample_selecting_a_sample")|tojson }}
            }, {
              "title": "Viewing reports",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_sample_viewing_reports")|tojson }}
            }, {
              "title": "Changing listing details",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_sample_change_listing_details")|tojson }}
            }, {
              "title": "Updating the model",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_sample_updating_the_model")|tojson }}
            }, {
              "title": "Tracking changes",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_sample_tracking_changes")|tojson }}
            }, {
              "title": "Configuring property settings",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_sample_property_settings")|tojson }}
            }
          ]
        }, {
          "collapsed": true,
          "title": "Uploading your first property",
          "items": [
            {
              "title": "Creating a property",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_first_create_a_property")|tojson }}
            },
            {
              "title": "Schedules and leasing rules",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_first_schedule_and_leasing_rules")|tojson }}
            }, {
              "title": "Spaces, uses and other revenues",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_first_space_and_uses_other_revenues")|tojson }}
            }, {
              "title": "Operating expenses and capital expenditures",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_first_operating_expenses_and_capital_expenditures")|tojson }}
            }, {
              "title": "Purchase information and financing",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_first_purchase_information_and_financing")|tojson }}
            }, {
              "title": "Analysis period and reversion information",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_first_analysis_period_and_reversion_information")|tojson }}
            }
          ]
        }, {
          "collapsed": true,
          "title": "Managing your account",
          "items": [
            {
              "title": "Viewing recent activity",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_account_recent_activity")|tojson }}
            }, {
              "title": "Updating your profile",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_account_updating_your_profile")|tojson }}
            }, {
              "title": "Changing account details",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_account_changing_account_details")|tojson }}
            }, {
              "title": "Configuring notifications",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_account_configure_notifications")|tojson }}
            }, {
              "title": "Tracking sessions",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_account_track_sessions")|tojson }}
            }, {
              "title": "Billing preferences",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_account_billing_preferences")|tojson }}
            }
          ]
        },
        {
          "collapsed": true,
          "title": "Managing your organization",
          "items": [
            {
              "title": "Creating an organization",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_organization_creating_an_organization")|tojson }}
            }, {
              "title": "Creating teams and access controls",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_organization_creating_teams")|tojson }}
            }, {
              "title": "Adding members and access controls",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_organization_adding_members")|tojson }}
            }, {
              "title": "Tracking activity",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_organization_tracking_activity")|tojson }}
            }, {
              "title": "Billing preferences",
              "href": "#",
              "selected": {{ (context_page_name == "get_getting_started_organization_billing_information")|tojson }}
            }
          ]
        },
        {
          "title": "Next steps",
          "href": "#",
          "selected": {{ (context_page_name == "get_getting_started_next_steps")|tojson }}
        },
      ],
      init() {
        for (entry of this.table_of_contents) {
          for (item of entry.items || []) {
            if (item.selected) {
              entry.collapsed = false
              break;
            }
          }
        }
      }
    }
  }
  </script>
  <div class="flex pt-7" x-data="menu()" style="height: calc(100vh - 5.35rem)">
    <aside class="hidden lg:block absolute sticky border-r border-gray-300 relative" :class="menu_open ? 'w-[32rem]' : 'w-16'">
      <div class="absolute z-10 absolute top-4 right-4 text-gray-500 hover:text-gray-800">
        <button type="button" @click="menu_open = !menu_open" x-show="menu_open" title="Hide table of contents" style="transform: rotate(180deg);">
        <svg class="inline-block h-6" viewBox="0 -960 960 960" fill="currentColor">
          <path d="M80-240v-480h80v480H80Zm560 0-57-56 144-144H240v-80h487L584-664l56-56 240 240-240 240Z"></path>
        </svg>
      </button>
        <button type="button" @click="menu_open = !menu_open" x-show="!menu_open" title="Show table of contents">
        <svg class="inline-block h-6" viewBox="0 -960 960 960" fill="currentColor">
          <path d="M80-240v-480h80v480H80Zm560 0-57-56 144-144H240v-80h487L584-664l56-56 240 240-240 240Z"></path>
        </svg>
      </button>
      </div>
      <div>
        <div>
          <ol class="text-base text-gray-700 px-4 flex flex-col gap-1 py-4 overflow-y-scroll" style="height: calc(100vh - 10.1rem)" x-show="menu_open">
            <template x-for="entry in table_of_contents">
              <li>
                <div x-show="entry.href" class="py-0.5 px-0.5">
                  <a :href="entry.href" x-text="entry.title" :class="entry.selected ? 'font-semibold text-blue-500' : ''" class="pl-6 text-gray-900 tracking-wide"></a>
                </div>
                <div x-show="entry.items">
                  <div class="flex place-items-center">
                    <button type="button" title="Expand" x-show="entry.collapsed" @click="entry.collapsed = !entry.collapsed" class="py-0.5">
                      <svg class="inline-block h-5 pb-0.5" viewBox="0 -960 960 960" fill="currentColor">
                        <path d="M504-480 320-664l56-56 240 240-240 240-56-56 184-184Z"></path>
                      </svg>
                      <span class="text-gray-900 tracking-wide pl-1" x-text="entry.title"></span>
                    </button>
                    <button type="button" title="Collapse" x-show="!entry.collapsed" @click="entry.collapsed = !entry.collapsed" class="py-0.5">
                      <svg class="inline-block h-5 pb-0.5" viewBox="0 -960 960 960" fill="currentColor">
                        <path d="M480-344 240-584l56-56 184 184 184-184 56 56-240 240Z"></path>
                      </svg>
                      <span class="text-gray-900 tracking-wide pl-1" x-text="entry.title"></span>
                    </button>
                  </div>
                  <ol class="pl-5 py-1.5 flex flex-col text-sm font-normal" x-show="!entry.collapsed">
                    <template x-for="sub_entry in entry.items">
                      <li><a :href="sub_entry.href" x-text="sub_entry.title" class="hover:text-gray-800 w-full block rounded-md px-4 py-1" :class="sub_entry.selected ? 'text-blue-600 bg-blue-100 font-medium tracking-wide' : ''"></a></li>
                    </template>
                  </ol>
                </div>
              </li>
            </template>
          </ol>
        </div>
      </div>
      <div class="absolute bottom-0 border-t border-gray-300 bg-white w-full h-12 flex place-items-center">
        <button type="button" title="Change langauge" class="px-4 ml-0.5 flex place-items-center gap-4">
          <svg class="inline-block h-3" viewBox="0 0 9600 4800">
	          <path fill="#d52b1e" d="m0 0h2400l99 99h4602l99-99h2400v4800h-2400l-99-99h-4602l-99 99H0z"></path>
	          <path fill="#fff" d="m2400 0h4800v4800h-4800zm2490 4430-45-863a95 95 0 0 1 111-98l859 151-116-320a65 65 0 0 1 20-73l941-762-212-99a65 65 0 0 1-34-79l186-572-542 115a65 65 0 0 1-73-38l-105-247-423 454a65 65 0 0 1-111-57l204-1052-327 189a65 65 0 0 1-91-27l-332-652-332 652a65 65 0 0 1-91 27l-327-189 204 1052a65 65 0 0 1-111 57l-423-454-105 247a65 65 0 0 1-73 38l-542-115 186 572a65 65 0 0 1-34 79l-212 99 941 762a65 65 0 0 1 20 73l-116 320 859-151a95 95 0 0 1 111 98l-45 863z"></path>
          </svg>
          <span class="font-medium tracking-wide" x-show="menu_open">Canada - English</span>
        </button>
      </div>
    </aside>
    <main class="flex flex-col place-items-center w-full overflow-y-scroll">
      <div class="flex flex-col px-6 lg:max-w-5xl">
        {% block article_content %}{% endblock article_content %}
        {% include "docs/cta.html.jinja2" %}
      </div>
    </main>
  </div>
{% endblock content %}
