    <link rel="stylesheet" href="{{ url_for('get_styles', version=css_version) }}">
    <script defer src="{{ url_for('js', path='/lib/alpinejs-3.14.1.min.js') }}"></script>
    <style>svg { display: inline-block; } [x-cloak] { display: none; }</style>
